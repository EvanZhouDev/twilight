{
	"name": "twilight-lang",
	"module": "index.js",
	"type": "module",
	"version": "2.0.0",
	"author": "EvanZhouDev",
	"license": "GPL-3.0",
	"description": "Twilight is a modern lambda calculus runtime and REPL.",
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"vitest": "^1.6.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/EvanZhouDev/twilight.git"
	},
	"homepage": "https://github.com/EvanZhouDev/twilight",
	"keywords": [
		"lambda",
		"calculus"
	],
	"scripts": {
		"format": "biome format . --write",
		"lint": "biome lint .",
		"fix": "biome check --apply .",
		"fix-unsafe": "biome check --apply-unsafe .",
		"build": "bun build ./cli/index.ts --compile --outfile ./bin/twilight"
	},
	"dependencies": {
		"@types/node": "^22.10.2",
		"chalk": "^5.4.0",
		"fast-levenshtein": "^3.0.0"
	},
	"bin": {
		"twilight": "bin/twilight"
	}
}
